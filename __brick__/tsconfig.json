{
  "compilerOptions": {
    "allowJs": true,
    "outDir": "../../dist/out-tsc",
    "module": "commonjs",
    "types": ["cypress", "node"],
    "sourceMap": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
  },
  "include": [
    "env.d.ts",
    "**/*.ts",
    "**/*.js",
    "src/index.d.ts",
    "cypress.config.ts",
    ".cypressrunnerrc.ts",
    "**/*.d.ts"
  ]
}
